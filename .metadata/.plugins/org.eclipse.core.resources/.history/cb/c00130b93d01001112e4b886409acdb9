import java.util.Scanner;

public class exercicio_six {
	public static void main(String[] args) {
		System.out.println("================================");
		try (Scanner sc = new Scanner(System.in)) {
			int number;
			System.out.println("Escolha um numero qualquer: ");
			number = sc.nextInt();
			if (number > 0 || number < 25) {
				System.out.println("Interfalo [0, 25] ");
			}
			if (number > 25 || number < 50) {
				System.out.println("Interfalo [25, 50] ");
			}
			if (number > 50 || number < 75) {
				System.out.println("Interfalo [50, 75] ");
			}
			if (number > 75 || number < 100) {
				System.out.println("Interfalo [75, 100] ");
			}
		}
	}
}
