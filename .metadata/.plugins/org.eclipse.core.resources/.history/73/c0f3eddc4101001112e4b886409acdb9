import java.util.Locale;
import java.util.Scanner;

public class exercicio_six {
	void main(String[] args) {
		System.out.println("================================");
		try (Scanner sc = new Scanner(System.in)) {
			Locale.setDefault(Locale.US);
			double number;
			System.out.println("Escolha um numero qualquer: ");
			number = sc.nextDouble();
			
			if (number < 0.00 || number > 25.00) {
				System.out.println("Fora de interfalo");
			}
			else if (number <= 25.0) {
				System.out.println("Interfalo [0, 25] ");
			}
			else if (number <= 50.0) {
				System.out.println("Interfalo [25, 50] ");
			}
			else if (number < 75.0) {
				System.out.println("Interfalo [50, 75] ");
			}
			else {
				System.out.println("Interfalo [75, 100] ");
			}
		}
	}
}
