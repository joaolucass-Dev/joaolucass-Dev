import java.util.Locale;
import java.util.Scanner;

public class exercicio_seven {
	public static void main(String[] args) {
		Locale.setDefault(Locale.US);
		try (Scanner sc = new Scanner(System.in)) {
			Double x;
			Double y;
			System.out.println("-------Coordenadas X & Y-------");
			System.out.println("Escolha dois numeros flootoantes: ");
			System.out.println("");
			
			System.out.println("X: ");
			x = sc.nextDouble();
			
			System.out.println("Y: ");
			y = sc.nextDouble();
			
			// 1º NÍVEL: O ponto mais especifico (ORIGEM)
			if (x == 0 || y == 0) {
				System.out.printf("Origem");
			}
			
			else if (x == 0) {
				System.out.printf("Eixo Y");

			}
			
			else if (y == 0) {
                System.out.println("Eixo X");
            }
			
			// 3º NÍVEL: O ponto não toca em nada (QUADRANTES)
            else if (x > 0 && y > 0) {
                System.out.println("Q1");
            } 
            else if (x < 0 && y > 0) {
                System.out.println("Q2");
            } 
            else if (x < 0 && y < 0) {
                System.out.println("Q3");
            } 
            else {
                // Se não é origem, nem eixo, nem Q1, Q2 ou Q3... só sobrou o Q4!
                System.out.println("Q4");
            }

		}
	}
}
