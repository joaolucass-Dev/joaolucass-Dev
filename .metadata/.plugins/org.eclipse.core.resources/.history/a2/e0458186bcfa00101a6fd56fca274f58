
public class One {
 void main(String[] args)
 {
	 int a;
	 int b;
	 
	 a = 10;
	 b = 30;
	 int soma = a + b;
	 System.out.printf("A soma de %d mais %d é igual a: %d", a,b, soma);
	 
	 a = -30;
	 b = 10;
	 soma = a + b;
	 System.out.printf("A subtração de %d menos %d é igual a: %d", a,b, soma);
	 
	 a = 0;
	 b = 0;
	 soma = a + b;
	 System.out.printf("A subtração de %d menos %d é igual a: %d", a,b, soma);
 }
}
