import java.util.Locale;
import java.util.Scanner;

public class exercicio_seven {
	public static void main(String[] args) {
		Locale.setDefault(Locale.US);
		try (Scanner sc = new Scanner(System.in)) {
			float x;
			float y;
			System.out.println("-------Coordenadas X & Y-------");
			System.out.println("Escolha dois numeros flootoantes: ");
			System.out.println("");
			System.out.println("X: ");
			x = sc.nextFloat();
			System.out.println("Y: ");
			y = sc.nextFloat();

			if (x == 0.0f || y == 0.0f) {
				System.out.printf("Origem");
			} else if (x >= 0.0f && y >= 0.0f) { // Q1
				System.out.printf("Q1");

			} else if (x <= -0.1f && y >= 100f) { // Q2
				System.out.printf("Q2");

			} else if (x <= -0.1f && y <= -100f) { // Q3
				System.out.printf("Q3");

			} else if (x >= 0.1f && y <= -0.1f) { // Q4
				System.out.printf("Q4");
			}

		}
	}
}
